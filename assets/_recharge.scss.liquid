        flex: 1;
        border: 0;
        box-shadow: none;
        max-width: 30%;
        padding: 0;
        text-align: center;
      }

      .date-toggle {
        position: relative;
        display: -ms-flexbox;
        display: flex;
        -ms-flex-align: center;
        align-items: center;
        -ms-flex: 1;
        flex: 1;
        margin-left: 6px;
        overflow: visible;
        border: 1px solid $account-pop-border-color;

        input {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          border: 0;
          padding-left: 42px;
          box-shadow: none;
          z-index: 1;
          background-color: transparent;
          color: transparent;

          &.is-active {
            color: $account-primary-color;
          }
        }

        span.text-primary {
          color: $account-primary-color;
          white-space: nowrap;
          overflow: hidden;
          text-overflow: ellipsis;
        }

        .icon {
          margin-right: 12px;
        }

        .qs-datepicker {
          position: absolute;
          top: 0;
          z-index: 1;
          border: 1px solid #e2e3e4;
          right: 0;
          left: auto!important;
          width: 100%;
          min-width: 250px;
          background-color: #fff;

          &.qs-hidden { display: none; }

          .qs-controls {
            .qs-arrow {
              &.qs-left {
                &:after {
                  border-right-color: $account-primary-color;
                }
              }

              &.qs-right {
                &:after {
                  border-left-color: $account-primary-color;
                }
              }
            }

            .qs-month-year {
              color: $account-primary-color;
              pointer-events: none;
            }
          }

          .qs-square {
            &.qs-active {
              background: $account-primary-color;
              color: #fff;
            }

            &:not(.qs-empty):not(.qs-disabled):not(.qs-day):not(.qs-active):hover {
              background: #add8e6;
            }
          }
        }
      }
    }

    .rc-pdp__form-submit {
      position: relative;
      background-color: $account-primary-color;
      color: #fff;
      opacity: 1;
      padding: 15px 30px;
      line-height: 1;
      border: 2px solid transparent;
      text-transform: uppercase;

      &:hover:not(.is-disabled) {
        @media screen and (min-width: 768px) {
          -ms-box-shadow: 0 5px 10px rgba(0,0,0,0.1);
          -webkit-box-shadow: 0 5px 10px rgba(0,0,0,0.1);
          box-shadow: 0 5px 10px rgba(0,0,0,0.1);
          color: $account-primary-color;
          background-color: #fff;
        }
      }

      &.is-disabled {
        background-color: #e6e9eb;
      }

      &:before, &:after {
        content: "";
        position: absolute;
        z-index: 1;
        opacity: 0;
      }

      &:before {
        top: 50%;
        left: 50%;
        transform: translate(-50%,-50%);
        background-repeat: no-repeat;
        background-position: 50%;
        background-size: contain;
        transition: opacity .3s cubic-bezier(.32,.36,.4,1);
      }

      &:after {
        background-color: $account-primary-loading-color;
        bottom: 0;
        left: 0;
        height: 6px;
        width: 100%;
      }

      &.is-loading:after {
        content: "";
        max-width: 90%;
        max-height: 6px;
        opacity: 1;
        transition: max-width 1.75s cubic-bezier(.32,.36,.4,1),max-height .25s cubic-bezier(.32,.36,.4,1),opacity .25s cubic-bezier(.32,.36,.4,1);
      }

      &.is-success:before {
        content: "";
        width: 15px;
        height: 12px;
        opacity: 1;
        background-image: url('icon-checkmark.svg');
      }

    }

  }
}

