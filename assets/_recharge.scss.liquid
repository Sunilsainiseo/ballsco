$account-primary-color: #015db9;
$account-loading-color: #e6e9eb;
$account-primary-loading-color: #3d87d0;
$account-pop-border-color: #9eccfb;


@keyframes btnLoading{0%{transform:translate(-50%,-50%) rotate(0deg)}to{transform:translate(-50%,-50%) rotate(359deg)}}@keyframes btnFinishLoading{0%{max-width:90%;max-height:6px;opacity:1}50%{max-width:100%;max-height:6px;opacity:1}to{max-width:100%;max-height:6px;opacity:0}}

.container-fluid {
  max-width: 1200px;
  margin-left: auto;
  margin-right: auto;
  padding: 20px;

  .dashbord_banner & {
    max-width: 100%;
    position: relative;
    padding: 0;
  }
}

hr.hr--small {
  border: none;
  margin: 25px 0;
  clear: both;
}

.rc-grid {
  list-style: none;
  margin: 0;
  padding: 0;

  &:after {
    content: '';
    display: table;
    clear: both;
  }

  @media screen and (max-width: 767px) {
    margin-left: 0;
  }

  &.is-loading {
    &:before {
      content: url(icon-loader-primary.svg);
      position: absolute;
      top: 50%;
      left: 50%;
      width: 20px;
      height: 20px;
      animation: btnLoading 1s infinite linear;
      transform: translate(-50%,-50%) rotate(0deg);
    }
  }
}

.rc-grid__item {
  box-sizing: border-box;
  float: left;
  min-height: 1px;
  padding-left: 30px;
  vertical-align: top;
  width: 100%;
}
.account-navigation__dropdown { 
  width: 100%; 
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  padding-right: 40px;
  background-image: url(icon-select.svg);
  background-position: calc(100% - 15px) 50%;
  background-repeat: no-repeat;
}

.rc-btn {

  &.is-loading {
    background-color: #fff;
    border-color: #d2d4d6;
    pointer-events: none;
    position: relative;

    &:after {
      content: "";
      max-width: 90%;
      max-height: 6px;
      opacity: 1;
      transition: max-width 1.75s cubic-bezier(.32,.36,.4,1),max-height .25s cubic-bezier(.32,.36,.4,1),opacity .25s cubic-bezier(.32,.36,.4,1);
    }
  }

  &:after {
    content: "";
    position: absolute;
    z-index: 1;
    opacity: 0;
    background-color: $account-loading-color;
    bottom: 0;
    left: 0;
    height: 6px;
    width: 100%;
    max-width: 0;
    max-height: 0;
    transition: max-width 1.75s cubic-bezier(.32,.36,.4,1),opacity .25s cubic-bezier(.32,.36,.4,1);
    animation-fill-mode: backwards;
  }
}

.btn.text-link {
  border: 0;
  padding: 0;
  background: none;
  box-shadow: none;
  -webkit-user-select: text;
  -moz-user-select: text;
  -ms-user-select: text;
  user-select: text;
  color: $account-primary-color;
  font-size: 13px;
  line-height: 1.3;
  text-decoration: underline;
  -webkit-text-decoration-color: #e6e9eb;
  text-decoration-color: #e6e9eb;
}

.rc-radio {
  position: relative;
  display: -ms-flexbox;
  display: flex;
  height: 52px;
  -ms-flex-align: center;
  align-items: center;
  padding: 0 16px;
  margin-bottom: 12px;
  border: 1px solid $account-pop-border-color;
  box-shadow: inset 0 3px 0 0 rgba(0,0,0,.02), 0 2px 0 0 rgba(0,0,0,.04);
}

.checkbox__input, .radio__input {
  position: absolute;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  opacity: 0;
  margin: 0;
  padding: 0;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
  cursor: pointer;
}

.checkbox__label, .radio__label {
  position: relative;
  display: -ms-flexbox;
  display: flex;
  color: #2e3133;
  font-size: 16px;
  line-height: 1;
  font-weight: 400;
  text-transform: none;
  margin-bottom: 0;

  &:before {
    content: "";
    width: 16px;
    height: 16px;
    margin-right: 8px;
    background-color: #fff;
    border-radius: 2px;
    border: 1px solid #d2d4d6;
    box-shadow: 0 2px 0 0 rgba(0,0,0,.04);
    transition: .3s cubic-bezier(.32,.36,.4,1);
  }
}

.radio__label:before {
  border-radius: 50%;
}

.rc-radio__label {
  &.radio__label {
    font-weight: 500;
    letter-spacing: 0;
  }

  &:after {
    content: "";
    width: 8px;
    height: 8px;
    border-radius: 50%;
    background-color: #fff;
    background-image: none;
    position: absolute;
    top: 50%;
    left: 4px;
    transform: translateY(-50%);
    opacity: 0;
    transition: opacity .3s cubic-bezier(.32,.36,.4,1);
  }
}

.checkbox__input.is-active~.checkbox__label, .checkbox__input.is-active~.radio__label, .checkbox__input:checked~.checkbox__label, .checkbox__input:checked~.radio__label, .radio__input.is-active~.checkbox__label, .radio__input.is-active~.radio__label, .radio__input:checked~.checkbox__label, .radio__input:checked~.radio__label {
  &:before {
    border-color: $account-primary-color;
    background-color: $account-primary-color;
  }

  &:after {
    opacity: 1;
  }
}

.btn.btn--round {
  border-radius: 24px;
}

.btn.rc-btn--small {
  padding: 12px 16px;
}

button.btn, button.btn--secondary, .rte button.btn--secondary {
  display: block;
  margin-left: auto;
  margin-right: auto;
}

[edit] {
  cursor: pointer;
}

