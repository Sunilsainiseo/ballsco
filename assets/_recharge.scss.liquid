        -ms-flex: 1;
        flex: 1;
        display: block;
        padding: 0;
        margin: 0;
        border: 0;
        box-shadow: none;
        opacity: .4;
        transition: opacity .25s cubic-bezier(.32,.36,.4,1);
        background-color: $account-primary-color;
        color: #fff;

        &:before {
          content: "";
          position: absolute;
          z-index: 1;
          opacity: 0;
          top: 50%;
          left: 50%;
          transform: translate(-50%,-50%);
          background-repeat: no-repeat;
          background-position: 50%;
          background-size: contain;
          transition: opacity .3s cubic-bezier(.32,.36,.4,1);
        }

        &:after {
          content: "";
          position: absolute;
          z-index: 1;
          opacity: 0;
          bottom: 0;
          left: 0;
          height: 6px;
          width: 100%;
          max-width: 0;
          max-height: 0;
          transition: max-width 1.75s cubic-bezier(.32,.36,.4,1),opacity .25s cubic-bezier(.32,.36,.4,1);
          animation-fill-mode: backwards;
        }

        &:hover { opacity: 1; }
      }

